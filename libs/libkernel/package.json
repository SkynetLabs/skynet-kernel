{
  "name": "libkernel",
  "version": "0.1.45",
  "author": "Skynet Labs",
  "description": "helper library to interact with skynet and the skynet kernel",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "lint": "prettier -l 'src' 'src-test' '*.js' '*.json' && eslint 'src' 'src-test' '*.js' '*.json'",
    "clean": "node ./clean.js",
    "deps": "npm install libskynet@latest && npm audit fix",
    "update-deps": "npm run clean && npm run deps && npm install",
    "update-lint": "prettier -w 'src' 'src-test' '*.js' '*.json' && eslint 'src' 'src-test' '*.js' '*.json' --fix",
    "update": "npm run update-deps && npm run update-lint",
    "test": "tsc && tsc --project tsconfig.test.json && node ./dist-test/test.js",
    "prepare": "npm run lint && npm run test"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "eslint": "^8.13.0",
    "prettier": "^2.6.2"
  },
  "dependencies": {
    "@skynetlabs/libskynet": "^0.0.48",
    "libskynet": "^0.0.63"
  }
}
